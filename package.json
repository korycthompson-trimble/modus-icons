    "stylelint-config-standard-scss": "^6.1.0",
    "svg-sprite": "^3.0.0-alpha1",
    "svg2ttf": "^4.0.0",
    "svgexport": "^0.4.2",
    "svgicons2svgfont": "^5.0.0",
    "svgo": "2.8.0",
    "ttf2eot": "^2.0.0",
    "ttf2woff": "^2.0.1",
    "ttf2woff2": "^5.0.0",
    "underscore": "^1.7.0",
    "url-join": "^1.1.0"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "files": [
    "dist",
    "!.DS_Store"
  ],
  "hugo-bin": {
    "buildTags": "extended"
  }
}
